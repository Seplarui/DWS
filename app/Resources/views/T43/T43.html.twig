{% extends "base.html.twig" %}

{% block title %}Práctica T43{% endblock %}

{% block stylesheets %}
	<style type="text/css">
		ul {list-style:none;}
	</style>
{% endblock %}

{% block body %}
	<h3>Vista del array en html</h3>
	{# Creamos una tabla para rellenarla con el array #}
	<ul>
		{# Creamos un bucle for para mostrar el array #}
		{% for key, value in data %}
			<li>{{ key }}: {{ value|e }}</li>
		{% endfor %}
	</ul>
{% endblock %}
{% block javascripts %}{% endblock %}